{
  "name": "parse-lfs-message",
  "version": "1.1.1",
  "description": "Convert LFS Message to UTF-8",
  "source": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "typings": "dist/index.d.ts",
  "repository": "git@github.com:simbroadcasts/parse-lfs-message.git",
  "author": "pbutcher <pbutcher93@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "format": "run-s \"format:no-write --write\"",
    "format:no-write": "prettier \"./**/*.{js,ts,md,json}\"",
    "format:check": "run-s \"format:no-write --check\"",
    "test": "jest",
    "example": "node example/index.js"
  },
  "dependencies": {
    "iconv": "^3.0.0"
  },
  "devDependencies": {
    "@types/iconv": "^3.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "jest": "^29.4.2",
    "jest-junit": "^15.0.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/"
  ]
}
